<?xml version="1.0" ?>
<!DOCTYPE extension SYSTEM "../extension.dtd">
<extension name="constants" version="1.0.0">
 <constant name="C_INT" type="int" value="23"/>
 <test>
  <code>echo C_INT;</code>
  <result>23</result>
 </test>
 
 <constant name="C_FLOAT" type="float" value="3.14"/>
 <test>
  <code>echo C_FLOAT;</code>
  <result>3.14</result>
 </test>

 <constant name="C_STRING" type="string" value="hello world!"/>
 <test>
  <code>echo C_STRING;</code>
  <result>hello world!</result>
 </test>
</extension>
